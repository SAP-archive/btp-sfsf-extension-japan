<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog id="_IDGenViewGroupDialog1"
		confirm="handleGroupDialogConfirm">
		<groupItems>
			<ViewSettingsItem text="{i18n>caseID}" key="caseID" selected="true" />
			<ViewSettingsItem text="{i18n>candidateID}" key="candidateID"/>
			<ViewSettingsItem text="{i18n>candidateName}" key="candidateName" />
			<ViewSettingsItem text="{i18n>candidateDivisionName}" key="candidateDivisionName" />
			<ViewSettingsItem text="{i18n>candidateDepartmentName}" key="candidateDepartmentName" />
            <ViewSettingsItem text="{i18n>candidatePositionName}" key="candidatePositionName" />
            <ViewSettingsItem text="{i18n>divisionName}" key="divisionName" />
            <ViewSettingsItem text="{i18n>departmentName}" key="departmentName" />
            <ViewSettingsItem text="{i18n>positionName}" key="positionName" />
		</groupItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>