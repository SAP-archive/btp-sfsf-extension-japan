<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <P13nDialog id="SettingDialog_P13nDialog" showReset="true" showResetEnabled="{path: '/ShowResetEnabled'}" ok="onOKSettingDialog" cancel="onCancelSettingDialog"
                reset="onReset" class="sapUiSizeCompact">
        <panels>
            <P13nColumnsPanel id="SettingDialog_P13nColumnsPanel" changeColumnsItems="onChangeColumnsItems">
                <items>
                    <P13nItem id="SettingDialog_P13nItem_1" columnKey="_IDGenColumn1" text="{i18n>caseId}" visible="true"/>
                    <P13nItem id="SettingDialog_P13nItem_2" columnKey="_IDGenColumn2" text="{i18n>candidateID}" visible="true"/>
                    <P13nItem id="SettingDialog_P13nItem_3" columnKey="_IDGenColumn3" text="{i18n>candidateName}" visible="true"/>
                    <P13nItem id="SettingDialog_P13nItem_7" columnKey="_IDGenColumn7" text="{i18n>nextDivisionName}" visible="true"/>
                    <P13nItem id="SettingDialog_P13nItem_8" columnKey="_IDGenColumn8" text="{i18n>nextDepartmentName}" visible="true"/>
                    <P13nItem id="SettingDialog_P13nItem_9" columnKey="_IDGenColumn9" text="{i18n>nextPositionName}" visible="true"/>
                    <P13nItem id="SettingDialog_P13nItem_4" columnKey="_IDGenColumn4" text="{i18n>currentDivisionName}" visible="false"/>
                    <P13nItem id="SettingDialog_P13nItem_5" columnKey="_IDGenColumn5" text="{i18n>currentDepartmentName}" visible="false"/>
                    <P13nItem id="SettingDialog_P13nItem_6" columnKey="_IDGenColumn6" text="{i18n>currentPositionName}" visible="false"/>
                </items>
            </P13nColumnsPanel>
        </panels>
    </P13nDialog>
</core:FragmentDefinition>